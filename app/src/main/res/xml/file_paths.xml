<?xml version="1.0" encoding="utf-8"?>
<!-- This file defines paths that FileProvider can use to share files.
     It's essential for securely getting the camera photo back into our app. -->
<paths>
    <!-- 'external-files-path' refers to the directory where your app stores
         private files on external storage. -->
    <external-files-path name="my_images" path="Pictures" />
</paths>
